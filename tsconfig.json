{
  "compilerOptions": {
    // "baseUrl": ".",
    "paths": {
      "@cy/*": ["./cypress/*"],
      "@cy-lib/*": ["./src/lib/*"],
      "@cy-pdf.ts-test/*": ["./src/pdf/pdf.ts-test/*"],
      "@fe-3rd/*": ["../pdf.ts/src/3rd/*"],
      "@fe-cpl/*": ["../pdf.ts/src/lib/compiling/*"],
      "@fe-edt/*": ["../pdf.ts/src/lib/editor/*"],
      "@fe-lib/*": ["../pdf.ts/src/lib/*"],
      "@fe-pdf.ts-src/*": ["../pdf.ts/src/pdf/pdf.ts-src/*"],
      "@fe-pdf.ts-test/*": ["../pdf.ts/src/pdf/pdf.ts-test/*"],
      "@fe-pdf.ts-web/*": ["../pdf.ts/src/pdf/pdf.ts-web/*"],
      "@fe-src/*": ["../pdf.ts/src/*"],
      "@wretch": ["../pdf.ts/src/3rd/wretch-2.9.0/dist/index.js"],
      "@xstate": ["../pdf.ts/src/3rd/xstate-5.13.2/xstate.ts"],
      "@zod": ["../pdf.ts/src/3rd/zod-3.23.8/lib/index.mjs"]
    },
    "rootDir": "..",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "types": [
      "cypress"
    ],

    "target": "ES2022",
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "skipLibCheck": true,

    "strict": true,
    "exactOptionalPropertyTypes": true,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "isolatedModules": true,

    "outDir": "gen",
    "sourceMap": true,

    "allowJs": true,
    "checkJs": false,

    "allowTsImport": true,
    "preprocessorFile": "../pdf.ts/src/global.ts",
    "unaliasImportPaths": true
  },

  // "files": [
  //   "src/pdf/slave.cy.ts"
  // ],
  "include": [
    "cypress/**/*.ts",
    "src/**/*.cy.ts",
    "../pdf.ts/src/3rd/wretch-2.9.0/**/*",
    "../pdf.ts/src/3rd/xstate-5.13.2/**/*"
  ]
}
